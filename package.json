{
	"name": "@deebeetech/deebee-dms",
	"private": false,
	"version": "1.0.0",
	"description": "Complex configurable data editor for Supabase",
	"homepage": "https://github.com/deebee-tech/deebee-dms",
	"license": "MIT",
	"type": "module",
	"author": {
		"name": "Sandy Weatherby",
		"email": "unicornbeachdiaries@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/deebee-tech/deebee-dms"
	},
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"clean": "tsx scripts/clean_project.ts && pnpm install && pnpm translate",
		"dev": "vite dev",
		"db:types": "npx supabase gen types typescript --local --schema deebee_dms > ./src/lib/database.types.ts",
		"docs:build": "vitepress build docs",
		"docs:dev": "vitepress dev docs --port=5174",
		"env:beta": "dotenv-vault pull beta .env -y",
		"env:dev": "dotenv-vault pull dev .env -y",
		"env:local": "dotenv-vault pull local .env -y",
		"env:prod": "dotenv-vault pull prod .env -y",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"machine-translate": "inlang machine translate --project project.inlang",
		"prepare": "svelte-kit sync || echo ''",
		"preview": "vite preview",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"test:unit": "vitest",
		"translate": "tsx scripts/clear_translations.ts && npx @inlang/cli@3.0.11 machine translate --project ./project.inlang",
		"translate:validate": "npx @inlang/cli@3.0.11 validate --project ./project.inlang"
	},
	"dependencyNotes": {
		"@inlang/cli": "Pin to 3.0.11 to avoid github issue https://github.com/opral/inlang-paraglide-js/issues/526"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.2.0",
		"@lucide/svelte": "^0.525.0",
		"@sentry/sveltekit": "^9.39.0",
		"@supabase/sentry-js-integration": "^0.3.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.51.0",
		"@deebeetech/array-helper": "^1.0.0",
		"@deebeetech/is-helper": "^1.0.1",
		"clsx": "^2.1.1",
		"cookie": "^1.0.2",
		"flag-icons": "^7.5.0",
		"js-cookie": "^3.0.5",
		"libphonenumber-js": "^1.12.10",
		"maska": "^3.2.0",
		"postgres": "^3.4.7",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"uuid": "^11.1.0",
		"valibot": "^1.1.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.1",
		"@eslint/js": "^9.31.0",
		"@inlang/cli": "3.0.11",
		"@playwright/test": "^1.54.1",
		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/adapter-node": "^5.2.13",
		"@sveltejs/kit": "^2.24.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^22.16.4",
		"@vitest/browser": "^3.2.4",
		"@vitest/coverage-v8": "^3.2.4",
		"daisyui": "^5.0.46",
		"dotenv": "^17.2.0",
		"dotenv-vault": "^1.27.0",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.11.0",
		"globals": "^16.3.0",
		"jsdom": "^26.1.0",
		"playwright": "^1.54.1",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"supabase": "^2.31.4",
		"svelte": "^5.36.4",
		"svelte-check": "^4.2.2",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.37.0",
		"vite": "^7.0.4",
		"vite-plugin-devtools-json": "^0.3.0",
		"vitepress": "^1.6.3",
		"vitest": "^3.2.4"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@sentry/cli",
			"@tailwindcss/oxide",
			"esbuild",
			"supabase",
			"svelte-preprocess"
		]
	}
}
