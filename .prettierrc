{
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"printWidth": 120,
	"semi": true,
	"singleQuote": false,
	"tabWidth": 3,
	"trailingComma": "all",
	"useTabs": true
}
